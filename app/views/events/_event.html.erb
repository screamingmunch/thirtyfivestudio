<div>
	<h3><%= event.title %></h3>
</div>

<div>
	<h3><%= event.start_time %></h3>
</div>

<div>
	<h3><%= event.end_time %></h3>
</div>

<div>
	<h3><%= event.description %></h3>
</div>

<% if current_user && current_user.admin  %>
	<%= link_to "Edit", edit_event_path(event) %>
<% end %>

<% if current_user && current_user.admin  %>
	<%= link_to "Delete", event_path(event), method: :delete %>
<% end %>